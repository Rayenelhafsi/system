{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.9.0"
    }
  },
  "routes": [
    {
      "src": "/$",
      "dest": "/api/index.php?path=index.php"
    },
    {
      "src": "/(.*\\.php)$",
      "dest": "/api/index.php?path=$1"
    }
  ]
}
